// JavaScript Document
          function openSnippet(){
          	window.open("http://www.infomous.com/site/mockups/FutureM/popup.html","_blank", "width=700,height=550,menubar=no,toolbar=no,resizable=no");
          }
          function openHelp(){
          	window.open("http://www.infomous.com/site/mockups/FutureM/infohelp.html","_blank", "width=800,height=400,menubar=no,toolbar=no,resizable=no");
          }
document.write('<a href="javascript:openHelp()"><img src="http://www.infomous.com/site/mockups/tnaflix/help_on_big.png"></a>\
		<a href="javascript:openSnippet()"><img src="http://www.infomous.com/site/mockups/tnaflix/popup_on_big.png"></a>');
