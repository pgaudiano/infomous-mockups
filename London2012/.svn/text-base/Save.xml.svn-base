  <!-- Sources: MISC -->
  <source id="810" request="999" url="http://www.infomous.com/"></source> <!-- test popup -->
  <source id="901" request="10"></source>    <!-- Personal -->
  <source id="902" request="20"></source>    <!-- Publishers -->

  <source id="999" request="999"></source>  <!-- Blank -->

</sources>
<!-- WORDS section -->
<words>

<!-- Infomous main -->
   <word id="0" text="Infomous" count="3.5" group="0" sources="802,800,30,10,20,40,50,9"/>

<!-- Personal -->
   <word id="10" text="Personal" count="3.1" group="0" sources="801,800,10,19"/>

<!-- Publishers -->
   <word id="20" text="Publishers" count="3.0" group="0" sources="803,800,20,29"/>

<!-- Advertisers -->
   <word id="30" text="Advertisers" count="2.9" group="0" sources="802,800,30,39"/>

<!-- Examples -->
   <word id="40" text="Gallery" count="3.0" group="0" sources="802,800,40,49"/>

<!-- Help -->
   <word id="50" text="  Help  " count="3.0" group="5" sources="802,800,50,59"/>

<!-- Contacts -->
   <word id="60" text="Contact" count="1.5" group="5" sources="802,800,61,62,63,60"/>

<!-- Sharing -->
   <word id="100" text="Share" count="3.0" group="10" sources="106,105,104,103,102,101,100"/>
