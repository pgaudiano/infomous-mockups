<div class="headline">
<h1 class="h2">Sample Infomous Spotlighters for ESPN.com</h1>
</div>
<hr>

<!-- Script for pop-up -->
  <script language="javascript">
          <!--
          function openSnippet(fname){
          	window.open(fname,"_blank", "width=800,height=650,menubar=no,toolbar=no,resizable=no");
