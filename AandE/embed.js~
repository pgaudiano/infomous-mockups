// JavaScript Document
          function openSnippet(){
          	window.open("http://www.infomous.com/site/mockups/AandE/index_full.html","_blank", "width=800,height=600,menubar=no,toolbar=no,location=no,resizable=yes,scrollbars=yes");
          }
          function openHelp(){
          	window.open("http://www.infomous.com/site/mockups/AandE/infohelp.html","_blank", "width=800,height=400,menubar=no,toolbar=no,resizable=no");
          }
document.write('<a href="javascript:openHelp()"><img src="http://www.infomous.com/site/mockups/AandE/help_on_big.png"></a>\
		<a href="javascript:openSnippet()"><img src="http://www.infomous.com/site/mockups/AandE/popup_on_big.png"></a>');
