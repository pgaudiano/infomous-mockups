			var cansubmit = 'no';
			}
			
			
			if(!jQuery('#optemail').val())
			{
			jQuery('#optemail').addClass('optbred');
			var cansubmit = 'no';
			}
			
			if(cansubmit == 'yes')
				jQuery('#optinformid').submit();				
			else
				e.preventDefault();
				return false;
		});
		
