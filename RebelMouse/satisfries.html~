    <script type="text/javascript">
        // Child pages should override performRedirect to false if there should be no redirection for this page.
        var performRedirect = true;
        var redirectTargetURL = null;

        function closeRedirectLightbox() {
            countdownCancelled = true;
            if (typeof triggerHomeContent == 'function') {
                triggerHomeContent("resume");
            }
            $('#redirectOverlay').hide();

            restaurantController.locateRestaurant();
        }

        function setRedirectExceptionCookie() {
            $.cookie("CountryRedirectException", "true", { expires: 0, path: '/' });
        }

        function onStayAtBKButtonClicked() {
            countdownCancelled = true;
            setRedirectExceptionCookie();
            closeRedirectLightbox();
        }

        function onContinueToMyCountrysSiteButtonClicked() {
            redirectToTargetURL();
        }

        function redirectToTargetURL() {
            if (redirectTargetURL != null) {
                window.location = redirectTargetURL;
            }
            closeRedirectLightbox();
        }

        function decrementRedirectCountdown() {
            if (!countdownCancelled) {
                $('#redirectCountdown').html(--countdownSeconds);
                if (countdownSeconds > 0) {
                    setTimeout(decrementRedirectCountdown, 1000);
                } else {
                    redirectToTargetURL();
                }
            }
        }

        $(document).ready(function () {
            ipGeolocator.registerCountryLocationResolutionCallback(function (countryLocation) {
                countryRedirectController.registerCheckCountryResolutionCallbackFn(function (defaultRedirect, boolRedirect, strWebsiteURL) {
                    if (!performRedirect || $.trim(boolRedirect) == "false") {
                        restaurantController.locateRestaurant();
                    } else {
                        if ($.cookie("CountryRedirectException") != null && $.trim($.cookie("CountryRedirectException")) == "true") {
                            setRedirectExceptionCookie();
                            restaurantController.locateRestaurant();
                        } else {
                            redirectTargetURL = strWebsiteURL;
                            $('#continueToMyCountrysSite').html('%{country} Website'.replace('%{country}', countryRedirectController.getCountryName()));
                            $('#redirectCountdown').html(countdownSeconds);
                            setTimeout(decrementRedirectCountdown, 1000);
                            $('#redirectOverlay').show();
                        }
                    }
                });
                countryRedirectController.checkCountryRedirect(countryLocation);
            });
            ipGeolocator.getCountryGeolocation();
			// utilities.js
			printCopyrightYear();
        });
    </script>
    <div id="redirectOverlay">
        <div class="redirectLightboxWrap">
