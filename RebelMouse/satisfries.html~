<script type="text/javascript">

    //protected bug on IE
    $("#niLocations div.navpane, #niCompanyInfo div.navpane, #niFreshOffers div.navpane, #niMenu div.navpane").css({
        'left': '-5000px',
        'visibility': 'visible',
        'display': ''
    });

    function initAreaBoxes(parent) {
        if (typeof (parent) == 'undefined')
            parent = $('ul#MainNavigation');

        $('.content', parent).each(function () {
            var areaBoxes = $('> .areaBox', this);
            areaBoxes.removeClass('first').removeClass('last');
            areaBoxes.eq(0).addClass('first');
            areaBoxes.eq(-1).addClass('last');

            $('> .areaBox > .areaBox', this).removeClass('first').removeClass('last');
            $('> .areaBox > .areaBox:first-child', this).addClass('first');
            $('> .areaBox > .areaBox:last-child', this).addClass('last');

            makeSameHeight(areaBoxes);
        });
    }

    initializeMenuNavigator();

    function initializeMenuNavigatorNoAjax() {

        var menuElements = "";
        var cat = $.parseJSON(menu_cat);
        for (i = 0; i < cat.length; i++) {
            var categoryId = cat[i].ID;
            var displayTitle = cat[i].DisplayTitle;

            var strCategory = toSeoFriendly(stripHtml(displayTitle) + " " + categoryId);

            menuElements += '<ul class="category areaBox">';
            menuElements += '<li class="root"><a href="' + CulturePrefix + 'menu-nutrition/' + strCategory + '/index.html" class="titleLink lnkNutrition category' + categoryId + ' first-list">' + displayTitle + '</a></li>';

            //menuElements += retrieveSubCategories(strCategory, categoryId, categoryDetailUrl);
            for (j = 0; j < cat[i].subcats.length; j++) {
                var subCategoryId = cat[i].subcats[j].ID;
                var title = cat[i].subcats[j].Title;
                var strSubCategory = toSeoFriendly(stripHtml(title) + " " + subCategoryId);
                menuElements += '<li><a href="' + CulturePrefix + 'menu-nutrition/' + strCategory + '/' + strSubCategory + '/index.html" class="lnkNutrition category' + subCategoryId + ' first-list">' + title + '</a></li>';
            }

            menuElements += '</ul>';
        }

        $(menuElements).insertBefore($("#pdfdownload"));

        $('ul#MainNavigation > li.navitem#niMenu .content a.titleLink').each(function () {
            if ($('img.arrow', this).length == 0) {
                $(this).append('<img class="arrow" src="/images/nav/link_arrow.png" border=0  width=5 height=9 />');
            }
        });

        initAreaBoxes($('ul#MainNavigation > li.navitem#niMenu'));
    }


    function initializeMenuNavigator() {

        if (typeof menu_cat != 'undefined') {
            initializeMenuNavigatorNoAjax();
            return;
        }


        $.ajax({
            url: '/cms' + CulturePrefix + 'cms_out/menu_nutrition/categories/all.xml',
            datatype: 'xml',
            type: 'get',
            success: function (data) {
                var menuElements = "";
                $(data).find('MenuCategory').each(function () {
                    var categoryId = $(this).attr('ID');
                    var displayTitle = $(this).children('DisplayTitle').text();
                    var categoryDetailUrl = $(this).children('DetailsUrl').text();

                    var strCategory = toSeoFriendly(stripHtml(displayTitle) + " " + categoryId);
                    menuElements += '<ul class="category areaBox">';
                    menuElements += '<li class="root"><a href="' + CulturePrefix + 'menu-nutrition/' + strCategory + '/index.html" class="titleLink lnkNutrition category' + categoryId + ' first-list">' + displayTitle + '</a></li>';
                    menuElements += retrieveSubCategories(strCategory, categoryId, categoryDetailUrl);
                    menuElements += '</ul>';
                });

                $(menuElements).insertBefore($("#pdfdownload"));

                $('ul#MainNavigation > li.navitem#niMenu .content a.titleLink').each(function () {
                    if ($('img.arrow', this).length == 0) {
                        $(this).append('<img class="arrow" src="/images/nav/link_arrow.png" border=0  width=5 height=9 />');
                    }
                });

                initAreaBoxes($('ul#MainNavigation > li.navitem#niMenu'));
            }
        });
    }

    function retrieveSubCategories(strCategory, rootCategoryId, categoryDetailUrl) {
        var menuElements = "";
        $.ajax({
            url: categoryDetailUrl,
            datatype: 'xml',
            type: 'get',
            async: false,
            success: function (data) {
                $(data).find('SubCategory').each(function () {
                    var subCategoryId = $(this).attr('ID');
                    var title = $(this).attr('Title');
                    var strSubCategory = toSeoFriendly(stripHtml(title) + " " + subCategoryId);

                    menuElements += '<li><a href="' + CulturePrefix + 'menu-nutrition/' + strCategory + '/' + strSubCategory + '/index.html" class="lnkNutrition category' + subCategoryId + ' first-list">' + title + '</a></li>';
                });
            }
        });
        return menuElements;
    }
</script>

<iframe id="frmCareersReq" src="" width="1" height="1" frameborder="0" scrolling="no" style="display: none"></iframe>


<script type="text/javascript">

    $(document).ready(function () {

        restaurantController.registerLocationResolutionCallback(function initMyMealYourBKInfo(objRestaurant, objIPGeolocatedLatLng) {
            freshOffersController.registerLoadCallback(function (promotionItems) {
                var featured = promotionItems.filter(function () { return this.IsNavFeatured && this.IsAvailableAtThisTime() && this.IsAvailableAtRestaurant(objRestaurant); })
                                             .first();
                if (featured.length > 0) {
                    // Need to set values in cases it was hide before.
                    $("ul#MainNavigation #niFreshOffers .content").css("width", "677px");
                    $("ul#MainNavigation #niFreshOffers .content > .areaBox.featured ").show();

                    featured = featured[0];
                    if (featured.NavFeaturedHtmlOverride != '') {
                        $('#headerFeaturedOffer').html(featured.NavFeaturedHtmlOverride);
                    } else {
                        $('#headerFeaturedOffer').html('<a href="' + featured.GetRefItemUrl() + '" target="' + featured.GetRefItemTarget() + '">' +
                                                         '<img src="' + featured.NavFeaturedImagePath + '" border="0" />' +
                                                       '</a>');
                    }

                    $('#headerFeaturedOffer .rbutton').rbutton();
                }
                else {
                    // Hide unused spaces
                    $("ul#MainNavigation #niFreshOffers .content").css("width", "194px");
                    $("ul#MainNavigation #niFreshOffers .content > .areaBox.featured ").hide();

                }

                initAreaBoxes($('ul#MainNavigation > li.navitem#niFreshOffers'));
            });
        });

        setTimeout(UpdateYourBkCrossDomain, 3000);
    });


    function UpdateYourBkCrossDomain() {
        $("#frmCareersReq").prop("src", "http://www.bkcareers.com/service/CookiesRequest.ashx?ybk=" + encodeURI($("#yourBKName").html()));
    }

</script>

<script type="text/javascript">

    $(document).ready(function () {

        //protected bug in IE9
        $("#niLocations div.navpane, #niCompanyInfo div.navpane, #niFreshOffers div.navpane, #niMenu div.navpane").css({
            'left': '-5000px',
            'visibility': 'visible',
            'display': ''
        });

        $('a.nolink').attr('href', '#');
        $('a.nolink').click(function (e) {
            e.preventDefault();
        });

        $(document).click(function (e) {
            var target = $(e.target);
            var parents = target.parents('li.navitem');

            if (parents.length <= 0) {
                $("ul#MainNavigation > li").each(function (k, v) {
                    if ($(v).hasClass("hover")) {
                        OnNavMenuUnClick($(v));
                    }
                });
            }
        });

        $("ul#MainNavigation > li.navitem").click(function (e) { //onmousein

            var target = $(e.target);

            if ($(this).hasClass("hover")) {
                if ((target.is('li.navitem > a.nolink') && target.parents('li.navitem').length > 0) || target.is('li.navitem')) {
                    OnNavMenuUnClick($(this));
                }
            } else {
                OnNavMenuClick($(this));
            }

        });

        function OnNavMenuClick(nav) {

            $("ul#MainNavigation > li.navitem").each(function (k, v) {
                if ($(v).hasClass("hover")) {
                    OnNavMenuUnClick($(v));
                }
            });

            nav.addClass("hover");
            nav.find("> a.nolink").addClass("nolink_actived");

            var navpane = $('div.navpane', nav);
            navpane.css('display', '');

            var cw = $('.content', navpane).outerWidth(false);
            var ch = $('.content', navpane).outerHeight(false);
            $('.border_topcenter', navpane).css('width', (cw - 10) + 'px');
            $('.border_bottomcenter', navpane).css('width', (cw - 10) + 'px');
            $('.border_contentleft', navpane).css('height', ch + 'px');
            $('.border_contentright', navpane).css('height', ch + 'px');

            var wMainNav = $('ul#MainNavigation').outerWidth(true);
            var wNavItem = nav.outerWidth(true);
            var wNavPane = navpane.outerWidth(true);
            var lNavItem = nav.position().left;
            var left;
            if (navpane.hasClass('full_width') || navpane.hasClass('align_left')) {
                left = 0;
            } else if (navpane.hasClass('align_right')) {
                left = wMainNav - wNavPane;
            } else {
                left = lNavItem + (wNavItem - wNavPane) / 2;
            }

            if (left < 0) {
                left = 0;
            } else if (left + wNavPane > wMainNav) {
                left = wMainNav - wNavPane;
            }

            navpane.css({
                'left': left + 'px',
                'visibility': 'visible'
            });

            var navpaneArrow = $('div.navpane > .arrow', nav);
            navpaneArrow.css({
                'left': (lNavItem + wNavItem / 2 - left - navpaneArrow.width() / 2) + 'px'
            });

            if (nav.hasClass('mapLoactions')) {
                if (!nav.hasClass('mapLoacted')) {
                    globalHeaderLocation.mapInitializeMap();
                    nav.addClass("mapLoacted");
                    $('.pre_itemLoading').css("display", "none");
                }
            }
        }

        function OnNavMenuUnClick(nav) {
            nav.removeClass("hover");
            $('div.navpane', nav).css("visibility", "hidden");
            $('div.navpane', nav).css("display", "none");
            nav.find("> a.nolink").removeClass("nolink_actived");
        }

        $("ul.dropdown li ul li:has(ul)").find("a:first").append(" &raquo; ");

        initAreaBoxes();

        window.setTimeout(function () {
            initAreaBoxes();
            $("ul#MainNavigation div.navpane").each(function () {
                // Border
                // - top left
                $(this).append("<div class='border_topleft'></div>");
                // - top center
                $(this).append("<div class='border_topcenter'></div>");
                // - top right
                $(this).append("<div class='border_topright'></div>");
                // - content left
                $(this).append("<div class='border_contentleft'></div>");
                // - content right
                $(this).append("<div class='border_contentright'></div>");
                // - bottom left
                $(this).append("<div class='border_bottomleft'></div>");
                // - bottom center
                $(this).append("<div class='border_bottomcenter'></div>");
                // - bottom right
                $(this).append("<div class='border_bottomright'></div>");
                // Arrow
                $(this).append("<div class='arrow'></div>");

                var cw = $('.content', this).outerWidth(false);
                var ch = $('.content', this).outerHeight(false);
                $('.border_topcenter', this).css('width', (cw - 10) + 'px');
                $('.border_bottomcenter', this).css('width', (cw - 10) + 'px');
                $('.border_contentleft', this).css('height', ch + 'px');
                $('.border_contentright', this).css('height', ch + 'px');
            });
        }, 500);

        if (location.href.contains('/menu-nutrition/')) {
            $('#niMenu').addClass('active');
        }
        if (location.href.contains('/fresh-offers/')) {
            $('#niFreshOffers').addClass('active');
        }
        if (location.href.contains('/company-info/')) {
            $('#niCompanyInfo').addClass('active');
        }
        if (location.href.contains('/restaurant-locator/')) {
            $('#niLocations').addClass('active');
        }

        var userAgent = navigator.userAgent.toLowerCase();
        $.browser = {
            version: (userAgent.match(/.+(?:rv|it|ra|ie|me)[\/: ]([\d.]+)/) || [])[1],
            chrome: /chrome/.test(userAgent),
            safari: /webkit/.test(userAgent) && !/chrome/.test(userAgent),
            opera: /opera/.test(userAgent),
            msie: /msie/.test(userAgent) && !/opera/.test(userAgent),
            mozilla: /mozilla/.test(userAgent) && !/(compatible|webkit)/.test(userAgent)
        };

        // Implement location top menu bar fix, for non-chrome browser
        if (!$.browser.chrome) {
            $("ul#MainNavigation li#niShare").next().css("padding-right", "2px");
        }
    });
</script>

<script type="text/javascript">
    var globalHeaderLocation = BK.globalHeaderLocation;
    var headerLocationHasGeocodedResult = false;

    function headerRestaurantLocatorOnKeyPress(control, e) {
        if (e.keyCode == 13 && $(control).val() != '') {
            // Use timeout to wait for form auto complete
            setTimeout(headerGetFormattedAddressFromGeocoder, 250);
            return false;
        } else {
            return true;
        }
    }

    function headerGetFormattedAddressFromGeocoder() {
        geocoder = new google.maps.Geocoder();
        geocoder.geocode({ 'address': $("#nav_find_bk").val(), 'region': CultureCountry },
                            function (response, status) {
                                if (status == google.maps.GeocoderStatus.OK) {
                                    $("#nav_find_bk").val(response[0].formatted_address);
                                }
                                headerLocationHasGeocodedResult = true;
                                $("#form_find_bk").submit();
                            });
    }

    $(document).ready(function () {
        restaurantController.registerLocationResolutionCallback(function () {
            globalHeaderLocation.initialize();
        });
        
        ipGeolocator.getCityGeolocationForRewards();
    });

    function OnSuccessgetCityGeolocationForRewardsCall(cityLocationValue) {
        var rewardsCityGeolocationList = $('#cphBody_GlobalHeader_hdnRewardsCityGeolocation').val();

        if ((rewardsCityGeolocationList.indexOf(cityLocationValue) != -1)
                && $('#cphBody_GlobalHeader_hdnRewardsCityGeolocation').val() != "") {
            $(".rewardsClass").show();
            $("ul#MainNavigation > li#niYourBK > form#mainNavBKTitle > span#mainNavBKTitleText").css("width", "123px");
        }
        else {
            $(".rewardsClass").hide();
            $("ul#MainNavigation > li#niYourBK > form#mainNavBKTitle > span#mainNavBKTitleText").css("width", "233px");
        }
    }

    $(window).load(function () {
        // Make share text center, for some machine
        var niMenu = $("ul#MainNavigation li#niMenu").offset().left;
        var niLocations = $("ul#MainNavigation li#niLocations").offset().left;
        var diff = niLocations - niMenu;

        if (diff < 813) {
            var node = $("ul#MainNavigation li#niShare a.nolink");
            node.css("padding-left", "0px");
            node.css("padding-right", "0px");
            node.css("text-align", "center");
            node.css("width", "54px");
        }

    });
</script>

