	<script type="text/javascript" language="JavaScript">
		 // <![CDATA[
				subNavList = new Playlist('subNav');
				subNavMan = new UrlPlaylistManager(subNavList);
				subNavMan.pane = new Pane("_layout");
				subNavMan.activeClass="navOver";
				subNavMan.inactiveClass="navOff";
				subNavMan.pane.scrape('/sectionSubNav?id=7238216&section=&urlBase=null');
				subNavATimer = null;
				subNavDTimer = null;
				subNavActive=false;

				
									subNavMan.addItem("section","/sectionSubNav?id=7238216&section=&urlBase=null");
									
									subNavMan.addItem("sectionus","/sectionSubNav?id=16360130&section=us&urlBase=null");
									
									subNavMan.addItem("sectioninternational","/sectionSubNav?id=16360143&section=international&urlBase=null");
									
									subNavMan.addItem("sectionpolitics","/sectionSubNav?id=7238258&section=politics&urlBase=null");
									
									subNavMan.addItem("sectionvideo","/sectionSubNav?id=9163734&section=video&urlBase=null");
									
									subNavMan.addItem("sectionblotter","/sectionSubNav?id=7238440&section=blotter&urlBase=null");
									
									subNavMan.addItem("sectionhealth","/sectionSubNav?id=7238407&section=health&urlBase=null");
									
									subNavMan.addItem("sectionentertainment","/sectionSubNav?id=7238287&section=entertainment&urlBase=null");
									
									subNavMan.addItem("sectionbusiness","/sectionSubNav?id=7238288&section=business&urlBase=null");
									
									subNavMan.addItem("sectiontechnology","/sectionSubNav?id=7238235&section=technology&urlBase=null");
									
									subNavMan.addItem("sectiontravel","/sectionSubNav?id=7238239&section=travel&urlBase=null");
									
			
			subNavShow = function(){
				window.setTimeout(function(){
					$('#siteSubNav').fadeIn(100, function(){});
					subNavMan.pane.resize();
				}, 100);
			 }
			
			subNavHide = function(){
				$('#siteSubNav').hide();
			}
			
			/* subnav timers */
			subNavReset = function(){
				subNavHide();
				subNavMan.playlist.gotoItem("section");
				subNavDTimer=null;
				subNavActive = false;
			};

			subNavReset();
			subNavMouseOut = function(){
				if(subNavATimer != null){
					window.clearTimeout(subNavATimer);
				}
				if(subNavDTimer==null){
					subNavDTimer = window.setTimeout(subNavReset,500);
				}
			};
			subNavMouseOver=function(){
				if(subNavDTimer!=null){
					window.clearTimeout(subNavDTimer);
					subNavDTimer=null;
				}
			};
			subNavGoto = function(obj){
				subNavMouseOver();
				var o = obj;
				var time = subNavActive ? 100:200;
				if(subNavATimer != null){
					window.clearTimeout(subNavATimer);
					subNavATimer = null;
				}
				subNavATimer = window.setTimeout(function(){
					subNavActive = true;
					subNavList.gotoItem(o.id);
					o=null;
					subNavShow();
				},time);
				subNavList.prepare(o.id);
				
			}
			subNavMan.pane.addMouseOut(subNavMouseOut);
			subNavMan.pane.addMouseOver(subNavMouseOver);
						
			
			// ]]>
		 </script>

</div>
		
	</div>
</div>


		<div class="ad_container"><script type="text/javascript">digGetAd("ThinBanner");</script></div>
		


