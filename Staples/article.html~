<script type="text/javascript">
	function Openflyer() {
		var flyerURL = 'http://weeklyad.staples.com/staples?sneakpeek=y&adref=HomepageHeader&.html=2';
		var flyerHome = 'null';
		var langParam, zipCode;
		
		flyerURL = flyerURL.replace(/&amp;/g,"&");
		if (flyerURL.indexOf("&", flyerURL.indexOf("language")) != -1) {
			langParam = flyerURL.substring(flyerURL.indexOf("language"), flyerURL.indexOf("&", flyerURL.indexOf("language")));
		} else {
			langParam = flyerURL.substring(flyerURL.indexOf("language"), flyerURL.length);
		}
		
		var cookieName = 'zipcode';
		cookies = STAPLES.Cookies.getCookies();
		try {
			zipCode = cookies[cookieName];	
		} catch(e) {
			zipCode = null;
		}
		
		if(zipCode != null) {
			flyerURL = flyerURL + "&postalcode=" + zipCode;
		}
		else {
			flyerURL = flyerHome + "&" + langParam;
		}
		window.location = flyerURL;
	}
</script>
		
