(function() {
  $(function() {
    // Set the default definition list label by the longest label
    var statsItems = $('#info-card-overview-content .overview-stats dt');
    var basicInfoItems = $('#info-card-overview-content dl dt');

    if(statsItems.length > 0 && basicInfoItems > 0) {
      statsItems.each(function() {
        var index = basicInfoItems.index(this);
        basicInfoItems.splice(index, 1);
      });

      var offsetWidth = 20;
      var maxWidthItem = _.max(basicInfoItems, function(item) { return $(item).outerWidth(); });
      var maxItemWidth = $(maxWidthItem).outerWidth() + offsetWidth;
      basicInfoItems.each(function() {
        var maxItemDD = $(this).next('dd');

        if(maxItemDD !== 'undefined') {
          maxItemDD.css('margin-left', maxItemWidth + 'px' );
        }
      });
    }

    // Hide description placeholder when showing details
    if ($('.details.definition-list dd').length != 0) {
      $("#description .placeholder").hide();
    }

    // GA Tagging
    var currentUser = "false";
    if(!currentUser) {
      acctSignInTagging();
      acctRegistrationTagging();
    }
    entityOverviewTagging("company");

    // Page Views
    if(true) {
      CB.PageViews.trackEntityView(entity_uuid)
    }
  });
})(jQuery);</script></html>
