    <script type="text/javascript">
        function reset(resetPage) {
            var btnMax = document.getElementById("btnMaximizeVideoplayer");
            var btnMin = document.getElementById("btnMinimizeVideoplayer");

            if (btnMax != null) { btnMax.style.visibility = "hidden"; }
            if (btnMin != null) { btnMin.style.visibility = "visible"; }

            var tf = document.getElementById('top_frame');
            var bf = document.getElementById('bot_frame');
            if (tf != null) {
                tf.height = "256px";
                tf.src = resetPage;
            }
            if (bf != null)
                bf.height = document.documentElement.clientHeight - 256;
        }

        function minWin(smallPage) {
            var btnMax = document.getElementById("btnMinimizeVideoplayer");
            var btnMin = document.getElementById("btnMaximizeVideoplayer");

            if (btnMax != null) { btnMax.style.visibility = "hidden"; }

            if (btnMin != null) { btnMin.style.visibility = "visible"; }

            var tf = document.getElementById('top_frame');
            var bf = document.getElementById('bot_frame');
            if (tf != null) {
                tf.height = "106px";
                tf.src = smallPage;
            }
            if (bf != null)
                bf.height = document.documentElement.clientHeight - 106;
        }

        function SignatureScripts() {
            // !! viewing preference
            var chkPreference = document.getElementById("chkSigPreference");
            var disply = $.cookie('Forums_DIS_S' + document.domain.toLowerCase());
            if (disply == 'false') {
                chkPreference.checked = false;
                $(".Discussion_UserSignature").css("display", "none");
            } else {
                chkPreference.checked = true;
                $(".Discussion_UserSignature").css("display", "block");
            }


            $("#chkSigPreference").bind("click", function (e) {

                if (chkPreference.checked) {
                    $(".Discussion_UserSignature").css("display", "block");
                    $.cookie('Forums_DIS_S' + document.domain.toLowerCase(), 'true', { expires: 365 });
                } else {
                    $(".Discussion_UserSignature").css("display", "none");
                    $.cookie('Forums_DIS_S' + document.domain.toLowerCase(), 'false', { expires: 365 });
                }
            });
        }

        function DiscussionSorting() {
            // !! viewing Discussion sorting preference
            var chkDiscussion = document.getElementById("chkDiscussionCreatedSortOrder");
            var displyDiscussionSort = $.cookie('Forums_DIS_DiscussionCreatedSort_' + location.hostname.toLowerCase());

            if (chkDiscussion != null) {
                if (document.domain.toLowerCase() == "weddingchannel.com") {
                    var trDiscussion = document.getElementById("trDiscussionCreatedSortOrder");
                    if (trDiscussion != null) { trDiscussion.style.display = "none"; }
                }
                else {
                    chkDiscussion.style.display = "block";
                    if (displyDiscussionSort == 'false') {
                        chkDiscussion.checked = false;
                    } else {
                        chkDiscussion.checked = true;
                        if (displyDiscussionSort == null) {
                            $.cookie('Forums_DIS_DiscussionCreatedSort_' + location.hostname.toLowerCase(), 'true', { expires: 365 });
                        }
                    }

                    $("#chkDiscussionCreatedSortOrder").bind("click", function (e) {

                        if (chkDiscussion.checked) {
                            $.cookie('Forums_DIS_DiscussionCreatedSort_' + location.hostname.toLowerCase(), 'true', { expires: 365 });
                        } else {
                            $.cookie('Forums_DIS_DiscussionCreatedSort_' + location.hostname.toLowerCase(), 'false', { expires: 365 });
                        }
                        window.location.reload();
                    });
                }
            }
        }        
    </script>

<div id="ctl00_headContent_JqueryScripts_ThreadsListScript">
    <script type="text/javascript">
        $(document).ready(function () {
            $(".ForumList_ForumSpecificTitleSearchTable").remove();
            DiscussionSorting();
        });
    </script>
