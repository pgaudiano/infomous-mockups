        function forumUserControlSearch() {
            var ddllist = document.getElementById('ctl00_middleContent_Search_searchOption');
            var itemName = ddllist.options[ddllist.selectedIndex].value;                       
                        
            var searchText = document.getElementById('plckSearchText1');            
                                                
            if (itemName == 'user')
            {                
                var host = window.location.host.toLowerCase().replace("forums", "community");                
                window.parent.location = "http://" + host + "/cs/ks/friends/default.aspx?s="+searchText.value;
                
            }
            else if (itemName == 'all') 
            {
                return gSiteLife.ForumSearch('1');
            } 
            else 
            {
                return gSiteLife.ForumSpecificForumSearch('1', itemName);                
            }           
        }
  </script>
  
    
