 Cufon.replace('#cufonDiv h3'); Cufon.replace('#cufonDiv h2'); $(document).ready(function() { $(".headers").css("visibility", "visible"); }); </script>
 <script language="javascript">
 $(document).ready( function() { var $target = $('#mapFrame'); var $frame = $('#postHolder'); loadPosts($frame); loadBackGround($target); setInterval( function() { $frame.children().each( function() { $(this).fadeOut('slow'); $(this).remove(); } ); loadPosts($frame); } , 15000); }); function loadBackGround($_target) { var $theTarget = $_target; var $hash = new hash(); for ($i = 0; $i < $backgroundsObj.length; $i++) { var $tempHash = new hash(); var $tempBack = $backgroundsObj[$i]; $tempHash.setItem('image', $tempBack["image"]); $tempHash.setItem('quadrants', $tempBack["quadrants"]); $hash.setItem('background_' + $i, $tempHash); }; var $back = new hash(); $back = $hash.getItem('background_0'); $($theTarget).css('background-image', 'url(' + $back.getItem('image') + ')'); } function loadPosts($_frame) { var $theFrame = $_frame; var $displayThese = new Array(); var $tempPostId, $arr2str; $theFrame.innerHTML = ""; for ($i = 0; $i < 3; $i++) { if ($displayThese.length == 0) { $displayThese.push(randomBound(0, $postsObj.length - 1)); } else { $arr2str = $displayThese.toString(); do { $tempPostId = randomBound(0, $postsObj.length - 1); } while ($arr2str.search($tempPostId) != -1) $displayThese.push($tempPostId); } var $tempPost = renderPost($i, loadHash($postsObj[$displayThese[$i]])); $($tempPost).hide(); $theFrame.append($tempPost); $($tempPost).fadeIn('slow'); $tempPostId = null; } return $displayThese; } function renderPost($index, $post) { var $postDiv = document.createElement('div'); $($postDiv).addClass('post').addClass('post' + $index); if ($post.hasItem('imageUrl')) { var $avatar = new Image(); $avatar.src = $post.getItem('imageUrl'); $postDiv.appendChild($avatar); } if ($post.hasItem('postUrl')) { var $postTitle = document.createElement('h4'); var $postUrl = document.createElement('a'); $postUrl.setAttribute("href", $post.getItem('postUrl')); $postUrl.innerHTML = $post.getItem('postTitle'); $postTitle.appendChild($postUrl); $postDiv.appendChild($postTitle); } if ($post.hasItem('userName')) { var $userName = document.createElement('div'); $userName.setAttribute("class", "postContent"); $userName.innerHTML = '<span class="postTitle" >
posted by: </span>
<a href="' + $post.getItem('profileLink') + '">
' + $post.getItem('userName') + '</a>
'; $postDiv.appendChild($userName); } return $postDiv; } function loadHash($post) { var $hash = new hash(); $hash.setItem('imageUrl', $post["image"]); $hash.setItem('postTitle', $post["posttitle"]); $hash.setItem('postUrl', $post["posturl"]); $hash.setItem('profileLink', $post["profilelink"]); $hash.setItem('userName', $post["username"]); return $hash; } function randomBound(l, u) { return Math.floor((Math.random() * (u - l + 1)) + l); } </script>
