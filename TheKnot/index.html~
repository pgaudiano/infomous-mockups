<script>
	function onKeyDownFindCouples1() {
		var fn = document.getElementById('WidgetLoader_ctl03_inputFindGift1').value,
			ln = document.getElementById('WidgetLoader_ctl03_inputFindGift2').value;
			if (fn === 'First name') {
				fn = '';
			}
			if (ln === 'Last name') {
				ln = '';
			}
			window.open('http://uw.thenest.com/pub/search/default.aspx?cm_mmc=Affiliate-_-TheKnot-_-HPSearch-_-HPSearch&a=994&rfn=' + fn + '&rln=' + ln);
	}
	(function () {
		if (typeof $ !== 'undefined' && typeof jQuery !== 'undefined' && $ === jQuery) {
			$('#couplesearch input').focus(function () {
				this.select();
			});
			$('.inputFindGift').keydown(function (e) {
				if (e.which === 13) {
					 onKeyDownFindCouples1();
				}
			});
			$('.btnFindGift').click(function (e) {
				e.preventDefault();
				onKeyDownFindCouples1();
			});
		}
	}());
</script>
